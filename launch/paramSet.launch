<launch>
<param name="/nxt/axis_length" value="0.07" />
<param name="/nxt/wheel_radius_l" value="0.025"/>
<param name="/nxt/wheel_radius_r" value="0.025"/>
<param name="/nxt/enc_per_turn_left" value="500"/>
<param name="/nxt/enc_per_turn_right" value="500"/>
<param name="/nxt/gear_ratio_left" value="0.965"/>
<param name="/nxt/gear_ratio_right" value="0.99"/>
<param name="/nxt/max_speed" value="65"/>
<param name="/nxt/min_speed" value="60"/>
<param name="/nxt/max_turn_speed" value="70"/>
<param name="/nxt/tol_trans" value="0.05"/>
<param name="/nxt/tol_rot" value="0.05"/>
<param name="/nxt/turning_adaption" value="0.5"/>

<node pkg="nxt_control" type="nxt_control_node" name="nxt_control" output="screen" respawn="true"/>
<!--<node pkg="brain" type="brain_node" name="brain" output="screen"/>-->

</launch>


